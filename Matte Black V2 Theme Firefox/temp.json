{
    "manifest_version": 2,
  
    "applications": {
      "gecko": {
        "id": "arc-dark-theme@afnankhan",
        "strict_min_version": "58.0"
      }
    },
  
    "name": "Arc Dark Theme",
    "version": "2018.9.24",
    "description": "Arc Dark theme for Firefox",
  
    "icons": {
      "48": "icon.svg",
      "96": "icon.svg"
    },
  
    "theme": {
      "colors": {
        "tab_line": "#5294E2",
        "textcolor": "rgba(207, 218, 231, 0.8)",
        "accentcolor": "#2F343F",
        "tab_loading": "#5294E2", // Firefox 60
        "icons_attention": "#5294E2", // Firefox 60
  
        "toolbar": "#383C4A",
        "toolbar_text": "#D3DAE3",
        "toolbar_top_separator": "rgba(38, 42, 51, 0.97)",
        "toolbar_bottom_separator": "#2B2E39",
        "toolbar_vertical_separator": "#2B2E39",
  
        "toolbar_field": "#404552",
        "toolbar_field_text": "#D3DAE3",
        "toolbar_field_focus": "rgba(255, 255, 255, 0.075)", // Firefox 61
        "toolbar_field_border": "#2B2E39", // Firefox 59
        "toolbar_field_separator": "rgba(255, 255, 255, 0.2)", // Firefox 59
        "toolbar_field_border_focus": "#2B2E39", // Firefox 61
  
        "button_background_hover": "rgba(255, 255, 255, 0.12)", // Firefox 60
        "button_background_active": "#5294E2", // Firefox 60
  
        "popup": "#404552", // Firefox 60
        "popup_text": "#D3DAE3", // Firefox 60
        "popup_border": "#2B2E39",
        "popup_highlight": "#5294E2", // Firefox 60
        "popup_highlight_text": "#E3EAF3", // Firefox 60
  
        "ntp_text": "#D3DAE3", // Firefox 63
        "ntp_background": "#404552", // Firefox 63
  
        "sidebar": "#404552", // Firefox 63
        "sidebar_text": "#D3DAE3", // Firefox 63
        "sidebar_border": "#2B2E39", // Firefox 64
        "sidebar_highlight": "#5294E2", // Firefox 63
        "sidebar_highlight_text": "#E3EAF3" // Firefox 63
      }
    }
  }
  